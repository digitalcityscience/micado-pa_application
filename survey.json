{
    "title": "Usability Test",
    "description": "Please, select your age",
    "pages": [
        {
            "name": "page1",
            "elements": [
                {
                    "type": "text",
                    "name": "id",
                    "title": "ID participant"
                },
                {
                    "type": "text",
                    "name": "age",
                    "title": "Please, select your age",
                    "inputType": "number",
                    "min": "16",
                    "max": "84"
                },
                {
                    "type": "radiogroup",
                    "name": "gender",
                    "title": "What is your gender?",
                    "choices": [
                        {
                            "value": "female",
                            "text": "Female"
                        },
                        {
                            "value": "male",
                            "text": "Male"
                        },
                        {
                            "value": "other",
                            "text": "Prefer not to say/other"
                        }
                    ]
                },
                {
                    "type": "radiogroup",
                    "name": "education",
                    "title": "What is the highest educational level you have completed?",
                    "choices": [
                        {
                            "value": "e1",
                            "text": "Less than secondary"
                        },
                        {
                            "value": "e2",
                            "text": "Secondary, upper secondary or vocational education and training (VET)"
                        },
                        {
                            "value": "e3",
                            "text": "University studies"
                        }
                    ]
                },
                {
                    "type": "radiogroup",
