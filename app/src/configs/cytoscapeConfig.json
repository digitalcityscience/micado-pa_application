{
  "style": [
    {
      "selector": "node",
      "style": {
        "background-image": "/statics/icons/Step1.svg",
        "background-fit": "contain",
        "border-opacity": 0,
        "width": "100px",
        "height": "100px",
        "background-opacity": 0,
        "text-valign": "bottom",
        "label": "data(title)"
      }
    },
    {
      "selector": "edge",
      "style": {
        "width": 2,
        "straight": "bezier",
        "line-color": "#0f0",
        "curve-style": "bezier",
        "target-arrow-color": "#0f0",
        "target-arrow-shape": "triangle",
        "arrow-scale": 2,
        "label": "data(description)"
        
      }
    },
    {
      "selector": ".eh-handle",
      "style": {
        "background-color": "red",
        "background-image": "/statics/icons/drag.svg",
        "width": 30,
        "height": 30,
        "shape": "ellipse",
        "overlay-opacity": 0,
        "border-width": 12,
        "border-opacity": 0
      }
    },
    {
      "selector": ".eh-hover",
      "style": {
        "background-color": "red"
      }
    },
    {
      "selector": ".eh-source",
      "style": {
        "border-width": 2,
        "border-color": "red"
      }
    },
    {
      "selector": ".eh-target",
      "style": {
        "border-width": 2,
        "border-color": "red",
        "target-arrow-shape": "triangle"
      }
    },
    {
      "selector": ".eh-preview",
      "style": {
        "label": "",
        "background-color": "red",
        "line-color": "red",
        "target-arrow-color": "red",
        "source-arrow-color": "red",
        "target-arrow-shape": "triangle"
      }
    },
    {
      "selector": ".eh-ghost-edge",
      "style": {
        "label": "",
        "background-color": "red",
        "line-color": "red",
        "target-arrow-color": "red",
        "source-arrow-color": "red",
        "target-arrow-shape": "triangle"
      }
    },
    {
      "selector": ".eh-preview, .eh-ghost-edge",
      "style": {
        "background-color": "red",
        "line-color": "red",
        "target-arrow-color": "red",
        "source-arrow-color": "red",
        "target-arrow-shape": "triangle"
      }
    }
  ],
  "layout": {
    "name": "breadthfirst",
    "avoidOverlap": true,
    "directed": true
  },
  "userZoomingEnabled": false
}